      "}",
      "",
      "export interface ${1:name}State {",
      "\t${2:property}: ${3:boolean}",
      "}",
      "",
      "const ${1:name}_INITIAL_STATE: ${1:name}State = {",
      "\t${2:property}: ${4:false}",
