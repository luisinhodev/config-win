      "export const ${1:name}Provider:FC<Props> = ({ children }) => {",
      "\tconst [state, dispatch] = useReducer(${1/(.*)/${1:/downcase}/}Reducer, ${1:name}_INITIAL_STATE)",
      "\t$0",
      "\treturn (",
      "\t\t<${1:name}Context value={{",
      "\t\t\t...state",
      "\t\t}}>",
      "\t\t\t{children}",
      "\t\t</${1:name}Context>",
      "\t)",
      "}"
    ]
